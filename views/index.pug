        .container.d-flex.align-items-center.flex-column
            // Masthead Avatar Image
            img.masthead-avatar.mb-5(src='assets/img/avataaars.svg', alt='')
            // Masthead Heading
            h1.masthead-heading.mb-0 Demo Asynchronous in JavaScript
            // Icon Divider
            .divider-custom.divider-light
                .divider-custom-line
                .divider-custom-icon
                    i.fas.fa-star
                .divider-custom-line
            // Masthead Subheading
            p.pre-wrap.masthead-subheading.font-weight-light.mb-0 Author: HaDNM
    section#demo.page-section.demo
        .container
            // Portfolio Section Heading
            .text-center
                h2.page-section-heading.text-secondary.mb-0.d-inline-block DEMO
            // Icon Divider
            .divider-custom
                .divider-custom-line
                .divider-custom-icon
                    i.fas.fa-star
                .divider-custom-line
            // Portfolio Grid Items
            .row.justify-content-center
